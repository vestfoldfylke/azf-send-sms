{
  "name": "azf-send-sms",
  "version": "2.1.1",
  "description": "HttpTriggered SMS sending with MyLink",
  "keywords": [
    "azure",
    "sms",
    "mylink"
  ],
  "type": "module",
  "main": "dist/src/functions/*.js",
  "homepage": "https://github.com/vestfoldfylke/azf-send-sms#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vestfoldfylke/azf-send-sms.git"
  },
  "bugs": {
    "url": "https://github.com/vestfoldfylke/azf-send-sms/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Rune Moskvil Lyng√•s",
    "email": "rune.moskvil.lyngas@vestfoldfylke.no",
    "url": "https://github.com/runely"
  },
  "scripts": {
    "lint": "biome check && tsc --noEmit --project tsconfig.json",
    "lint:fix": "biome check --write",
    "build": "tsc --project tsconfig.json",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "test": "npm run lint && node --import tsx --test tests/**/*.test.ts"
  },
  "dependencies": {
    "@azure/functions": "^4.9.0",
    "@vestfoldfylke/loglady": "^1.3.0",
    "@vestfoldfylke/vestfold-metrics": "^1.0.0",
    "fluentvalidation-ts": "^5.0.0",
    "lru-cache": "^11.2.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
